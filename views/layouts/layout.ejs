<!DOCTYPE html>
<html lang="en">
   <head>
     <link rel='stylesheet' href='/css/style.css'> 
     <link rel='stylesheet' href='/css/bootstrap.min.css'>
     <link rel="stylesheet" href="/css/fontawesome.min.css">
    <title>zeestore</title>
   </head>
   <body>
      <%- include('../partials/header'); %>      
      <div class="colorBody">
         <div class="containerButton">
           <button id="redbutton">Reds</button>
            <button id="bluebutton">Blues</button>
             <button id="greenbutton">Greens</button>
              <button id="otherbutton">Others</button>
         </div>
      </div>
      <div class="productContainer">
        
         <div class="listing">
         
           <div class="listingImg">
            <a href="products/<% =products[i].id%>">
             <img src="#" /></a>
           </div>
           
            <div class="listingTitle">
            <a href="products/<% =products[i].id%>">
              <p>product title</p></a>
            </div>
            
            <div class="listingPrice">
              <p>$10</p>
            </div>
            <div class="listingButton">
              <button id="buyNow">Buy it now</button>
              <button id="addCart" value="<% =products[i].id"%>>Add to Cart</button>
            </div>
         </div>
      </div>
     <main>    
     <%-body%>      
     </main>
     
     <%- include('../partials/footer'); %>      
      <!-- js -->
      <script src='/js/bootstrap.bundle.min.js'></script>
      <script src='/js/all.min.js'></script>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
      <script src="/js/jquery.js"></script>
   </body>
</html>