<header>
  <div class="navigation">
    <div class="navlink">
      <a href="products">Home</a>
    </div>
    <div class="navlink">
      <a href="/colors">Colors</a>
    </div>
    <div class="navlink">
      <a href="/cart"
        >Cart<em> <% if (cartNumb != 0) { %> <%= cartNumb %> <% } %> </em></a
      >
    </div>
    <div class="navlink">
      <a href="/contact">Contact</a>
    </div>
  </div>
</header>
<div class="listingPageContainer container-fluid">
  <div class="listingImg">
    <img src="<%= product.cover%>" class="img-fluid" />
  </div>
  <div class="rightContents p-2">
    <div class="">
      <p id="listingTitle"><%= product.title%></p>
    </div>
    <div class="">
      <p id="price"><%= product.price%></p>
    </div>
    <div class="listingButtons">
      <button id="buyNow">Buy it now</button
      ><button id="addCart" value="<%= product.id%>">Add to Cart</button>
    </div>
    <div class="description">
      <p class=""><%= product.description%></p>
    </div>
    <div class="">
      <p class=""><i>Shipping Time- 4-5 Days</i></p>
    </div>
  </div>
</div>
